{
  "name": "xgbnl/elephant",
  "description": "LayeredStructure",
  "type": "library",
  "require": {
    "php": "^8.2",
    "ext-redis": "*",
    "xgbnl/paginator": "dev-main"
  },
  "require-dev": {
    "illuminate/support": "^10.13.5",
    "illuminate/database": "^10.13.5",
    "illuminate/http": "^10.13.5",
    "illuminate/redis": "^10.13.5",
    "phpunit/phpunit": "^10.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Elephant\\": "src/",
      "Elephant\\Support\\Facades\\": "src/Facades/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Elephant\\ServiceProvider"
      ],
      "aliases": {
        "RedisClient": "Elephant\\Facades\\RedisClient",
        "HttpResponder": "Elephant\\Facades\\HttpResponder"
      }
    }
  },
  "minimum-stability": "stable"
}
